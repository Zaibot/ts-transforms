{
  "name": "@zaibot/ts-transforms",
  "version": "0.1.2",
  "description": "TypeScript transforms",
  "author": {
    "name": "Tobias de Groen",
    "email": "tdgroen@zaibot.net"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepare": "npm test && npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zaibot/ts-transforms.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zaibot/ts-transforms/issues"
  },
  "homepage": "https://github.com/Zaibot/ts-transforms#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "node"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "\\.spec\\.ts$"
  },
  "dependencies": {
    "ts-simple-ast": "^9.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  }
}
